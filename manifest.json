{
    "minimum_chrome_version": "90",  	
    "short_name": "Project Deserve",	
    "name": "Project Deserve",
    "version": "0.0.1",
	"homepage_url": "https://comitfs.com",	
    "description": "Everyyone deserves good healthcare",
    "manifest_version": 3,
    "permissions": [
		"contextMenus"
    ],	
	"content_scripts": [
	  {
		"matches": [
			"https://github.com/project-deserve/*",
			"https://github.com/orgs/project-deserve/*",
			"https://github.com/organizations/project-deserve/*"
		],
		"js": ["./webcomponents-bundle.js", "./tingle.min.js", "./libsignal-protocol.min.js", "./converse.js", "./emojis.js", "./packages/jitsimeet/jitsimeet.js", "./packages/actions/actions.js", "./packages/location/location.js", "./packages/muc-info/muc-info.js", "./extension.js"],
		"css": ["./converse.css", "./tingle.min.css", "./packages/muc-info/muc-info.css"]
	  }
	],				
    "background": {
    	"service_worker": "./background.js"
  	}, 		
	"action": {
      "default_icon": {
		"192": "./img/logo_192.png",
		"32": "./img/logo_32.png"
      }   
	},	
	"icons": {
		"192": "./img/logo_192.png",
		"32": "./img/logo_32.png"
	},		
    "default_locale": "en",	
    "incognito": "not_allowed"
  }